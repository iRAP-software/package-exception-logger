{
    "name": "irap/exception-logger",
    "type": "library",
    "description": "Package for capturing and logging uncaught exceptions.",
    "keywords": ["error", "exception", "logging"],
    "homepage": "http://www.irap.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "support@irap.org",
            "homepage": "http://www.irap.org/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "irap/logging": "2.2.*"
    },
    "require-dev": {
        "irap/autoloader": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "iRAP\\ExceptionLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iRAP\\ExceptionLogger\\Testing\\": "testing"
        }
    }
}
